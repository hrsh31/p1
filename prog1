#PROBLEM 2
a = [int(x) for x in input().split()]
for i in a:
	if i<5:
		print(i)
    #problem3 
a = [int(x) for x in input().split()]
ele = int(input("Enter element:"))
count = -1;
for i in a:
	count = count + 1;
	if ele == i:
		print(count)
    #problem 4
duplicate = [int(x) for x in input().split()]
final_list = [] 
for num in duplicate: 
  if num not in final_list: 
    final_list.append(num) 
print(final_list)
#problem 5
str = input()
print(str[::-1])
#problem 6
d = dict()
for i in range(1,4):
  name = input("Enter name : ")
  usn  = input("Enter usn :")
  d[usn] = name
print(d)
